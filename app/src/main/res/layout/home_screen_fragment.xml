<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="16dp">

    <ImageView
        android:id="@+id/imgDIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/dicon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"/>

    <TextView
        android:id="@+id/txtTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Co-Working"
        android:textSize="16dp"
        android:fontFamily="@font/poppins_medium"
        app:layout_constraintLeft_toRightOf="@+id/imgDIcon"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnBookingHistory"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"

        android:background="@drawable/button_backgroud_shape"
        android:backgroundTint="@color/blue"
        android:fontFamily="@font/poppins_medium"
        android:text=" Booking History "
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgRectangle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@+id/btnBookingHistory"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/rectangle"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:src="@drawable/vector"
        app:layout_constraintTop_toTopOf="@+id/imgRectangle"
        app:layout_constraintTop_toBottomOf="@+id/btnBookingHistory"
        tools:layout_editor_absoluteX="35dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_regular"
        android:gravity="center"
        android:layout_marginStart="20dp"
        android:text="Book Work Station"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgRectangle" />

    <ImageView
        android:id="@+id/imgRectangle2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        app:layout_constraintLeft_toRightOf="@+id/imgRectangle"
        app:layout_constraintTop_toBottomOf="@+id/btnBookingHistory"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@drawable/rectangle2"/>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="24dp"
        android:src="@drawable/meetingroom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imgRectangle2"
        app:layout_constraintTop_toBottomOf="@+id/btnBookingHistory" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/poppins_regular"
        android:gravity="center"
        android:text="Meeting Room"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgRectangle2" />


</androidx.constraintlayout.widget.ConstraintLayout>